{
    "name": "hotrush/laravel-log-notification-channel",
    "description": "Log notifications channel for laravel",
    "homepage": "https://github.com/hotrush/laravel-log-notifications-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "hotrush",
            "email": "johny64@gmail.om",
            "homepage": "https://github.com/hotrush",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/notifications": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Log\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Log\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
